,CAA0BI,eAA9B;AAAA,CAhBV,CAAb;AAmBA,IAAMC,GAAG,GAAGjC,MAAM,CAACqB,GAAV,oBAAT;AASA,IAAMa,KAAK,GAAGlC,MAAM,CAACqB,GAAV,oBAAX;AAIA,IAAMc,KAAK,GAAGnC,MAAM,CAACC,EAAD,CAAT,qBAMA,UAAAwB,KAAK;AAAA,SAAIA,KAAK,CAACC,KAAN,CAAYC,MAAZ,CAAmBC,MAAnB,CAA0BQ,eAA9B;AAAA,CANL,CAAX;AAQA,IAAMC,IAAI,GAAGrC,MAAM,CAACE,CAAD,CAAT,sBAGC,UAAAuB,KAAK;AAAA,SAAIA,KAAK,CAACC,KAAN,CAAYC,MAAZ,CAAmBC,MAAnB,CAA0BU,cAA9B;AAAA,CAHN,CAAV,C,CAQA;;;;;;;;;;;0BA5EMlB,K;0BAYAG,O;0BAGAC,U;0BAaAM,O;0BAmBAG,G;0BASAC,K;0BAIAC,K;0BAQAE,I","sourcesContent":["import * as React from 'react';\nimport styled from '../../../themes/styled';\nimport CollectionType from '../../../types/Collection';\n// import { Link } from 'react-router-dom';\n// import Join from './Join';\nimport H5 from '../../typography/H5/H5';\nimport P from '../../typography/P/P';\nimport { Resource, Eye, Message } from '../Icons';\nimport media from 'styled-media-query';\nconst PlaceholderImg = require('../Icons/collectionPlaceholder.png');\n\ninterface CollectionProps {\n  collection: CollectionType;\n}\n/**\n * Collection component.\n */\n\nexport default ({ collection }: CollectionProps) => {\n  return (\n    <Wrapper>\n      {/* <Link\n        to={\n          collection.localId\n            ? `/collections/${collection.localId}`\n            : `/collections/federate?url=${encodeURI(collection.id)}`\n        }\n      > */}\n        <Img\n          style={{\n            backgroundImage: `url(${collection.icon || PlaceholderImg})`\n          }}\n        />\n        <Infos>\n          <Title>\n            {collection.name.length > 80\n              ? collection.name.replace(/^(.{76}[^\\s]*).*/, '$1...')\n              : collection.name}\n          </Title>\n          <Desc>\n            {collection.summary.length > 320\n              ? collection.summary.replace(\n                  /^([\\s\\S]{316}[^\\s]*)[\\s\\S]*/,\n                  '$1...'\n                )\n              : collection.summary}\n          </Desc>\n          <Actions>\n            <ActionItem>\n              {(collection.resources && collection.resources.totalCount) || 0}{' '}\n              <Resource\n                width={18}\n                height={18}\n                strokeWidth={2}\n                color={'#8b98a2'}\n              />\n            </ActionItem>\n            <ActionItem>\n              {(collection.followers && collection.followers.totalCount) || 0}{' '}\n              <Eye width={18} height={18} strokeWidth={2} color={'#8b98a2'} />\n            </ActionItem>\n            <ActionItem>\n              {(collection.threads && collection.threads.totalCount) || 0}{' '}\n              <Message\n                width={18}\n                height={18}\n                strokeWidth={2}\n                color={'#8b98a2'}\n              />\n            </ActionItem>\n          </Actions>\n        </Infos>\n      {/* </Link> */}\n      <Right>\n        {/* <Join\n          followed={collection.followed}\n          id={collection.localId}\n          externalId={collection.id}\n        /> */}\n      </Right>\n    </Wrapper>\n  );\n};\n\nconst Right = styled.div`\n  width: 38px;\n  margin-left: 16px;\n  ${media.lessThan('medium')`\n  margin-top: 8px;\n  background: #f7f8f9;\n  border-radius: 6px;\n  display: inline-block;\n  margin-left: 8px;\n`};\n`;\n\nconst Actions = styled.div`\n  margin-top: 10px;\n`;\nconst ActionItem = styled.div`\n  display: inline-block;\n  font-size: 14px;\n  font-weight: 600;\n  color: ${props => props.theme.styles.colour.collectionIcon};\n  text-transform: uppercase;\n  margin-right: 20px;\n  & svg {\n    vertical-align: sub;\n    color: inherit !important;\n  }\n`;\n\nconst Wrapper = styled.div`\n  display: flex;\n  cursor: pointer;\n  position: relative;\n  padding: 20px;\n  border-bottom: 1px solid ${props => props.theme.styles.colour.divider};\n  ${media.lessThan('medium')`\n  display: block;\n`} & a {\n    display: flex;\n    color: inherit;\n    text-decoration: none;\n    width: 100%;\n    flex: 1;\n  }\n  &:hover {\n    background: ${props => props.theme.styles.colour.collectionHover};\n  }\n`;\nconst Img = styled.div`\n  width: 120px;\n  height: 120px;\n  border-radius: 8px;\n  background-size: cover;\n  background-repeat: no-repeat;\n  margin-right: 8px;\n  margin-left: 8px;\n`;\nconst Infos = styled.div`\n  flex: 1;\n  margin-left: 8px;\n`;\nconst Title = styled(H5)`\n  font-size: 16px !important;\n  margin: 0 0 8px 0 !important;\n  line-height: 20px !important;\n  letter-spacing: 0.8px;\n  font-weight: 600;\n  color: ${props => props.theme.styles.colour.collectionTitle};\n`;\nconst Desc = styled(P)`\n  margin: 0 !important;\n  font-size: 14px !important;\n  color: ${props => props.theme.styles.colour.collectionNote};\n  font-size: 48px;\n  line-height: 20px;\n`;\n\n// export default Collection;\n"]}]}